{
  "name": "sciline-calculator",
  "version": "0.1.0",
  "scripts": {
    "build": "bsb -make-world",
    "start": "bsb -make-world -w",
    "clean": "bsb -clean-world",
    "bundle": "webpack -p ./src/SciLine.bs.js --output-library=SciLine",
    "test": "jest"
  },
  "keywords": [
    "BuckleScript"
  ],
  "author": "",
  "license": "MIT",
  "devDependencies": {
    "bs-platform": "^4.0.18",
    "cartesian": "^1.0.1",
    "eslint": "^5.12.0",
    "eslint-config-airbnb-base": "^13.1.0",
    "eslint-config-prettier": "^3.3.0",
    "eslint-plugin-import": "^2.14.0",
    "jest": "^23.6.0",
    "jest-silent-reporter": "^0.1.1",
    "lodash": "^4.17.11",
    "mathjs": "^5.4.0",
    "webpack": "^4.28.4",
    "webpack-cli": "^3.2.1"
  },
  "dependencies": {
    "big-rat": "^1.0.4",
    "bn.js": "^4.11.8"
  },
  "jest": {
    "reporters": [
      [
        "jest-silent-reporter",
        {
          "useDots": true
        }
      ]
    ],
    "collectCoverageFrom": [
      "src/**/*.bs.js",
      "!src/__mocks__/**/*.js"
    ]
